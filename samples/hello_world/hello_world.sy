
dec printf: func (fmt: @[byte], ...) returns byte;
dec fact  : func (num:   byte);

def abc as four := 5;

sub fact(num: four) returns four
    def f as four;
begin
    if num == 1 then
        f := 1;
    else
        f := num*fact(num-1);
    endif;
    return f;
end;

sub main(argc: four, argv: @[@[byte]]) returns byte
    def x  as four := 1;
    def y  as eigh := 1;
    def ax as twin in register("%di") := 3;
begin
    printf("Hello world! %x\n", unsigned(x>>2));
    return 0;
end;
